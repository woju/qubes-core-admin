compadd "$@" $(python -c '
import qubes.qubes
qvmc = qubes.qubes.QubesVmCollection()
qvmc.lock_db_for_reading()
qvmc.load()
qvmc.unlock_db()
print("\n").join(vm.name for vm in qvmc.values()
  if vm.is_qrexec_running())
')

# vim: ts=2 sw=2 et
