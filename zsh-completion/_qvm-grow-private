# All rights reserved. Licensed under GPL-2 or later.
#

_qvm-grow-private() {
  _arguments -s : \
    '(- : *)'{-h,--help}'[show help message and exit]' \
    ':Qubes VM:_qubes_vm_all' \
    ':size: ' \
  && return 0
}

_qvm-grow-private "$@"

# vim: ts=2 sw=2 et
